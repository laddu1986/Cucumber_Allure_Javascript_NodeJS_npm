{
  "name": "webdriverio-Test-Automation",
  "version": "1.0.0",
  "description": "WebdriverIO (examples and sample site)",
  "main": "index.js",
  "scripts": {
    "start": "http-server sample-site -p 8303",
    "test": "node node_modules/webdriverio/bin/wdio ./test/config/wdio.conf.js",
    "node-inspector": "node-inspector --debug-port 5859 --no-preload",
    "selenium-postinstall": "node node_modules/.bin/selenium-standalone install",
    "selenium-start": "node_modules/.bin/selenium-standalone start",
    "test-mobile": "node node_modules/webdriverio/bin/wdio ./test/config/suite.appium.conf.js",
    "allure-report": "node_modules/.bin/allure generate ./allure-results -c && allure open",
    "junit-report": "node_modules/.bin/junit-viewer --results=./test/reports/junit-results --save=junit-results.html && open junit-results.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/klamping/webdriverio-course-content.git"
  },
  "author": "Laddu shashavali",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/klamping/webdriverio-course-content/issues"
  },
  "homepage": "https://github.com/klamping/webdriverio-course-content#readme",
  "dependencies": {
    "allure-commandline": "^2.5.0",
    "appium": "^1.6.3",
    "assert": "^1.4.1",
    "babel-preset-env": "^1.6.0",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "cucumber": "^3.0.0",
    "cucumber-tag-expressions": "^2.0.1",
    "graceful-fs": "^4.1.4",
    "grunt": "^0.4.5",
    "grunt-webdriver": "^1.0.0",
    "http-server": "^0.11.1",
    "junit-viewer": "^4.11.1",
    "lodash": "^4.17.4",
    "node-excel-to-json": "0.0.3",
    "node-inspector": "^1.1.1",
    "underscore": "^1.8.3",
    "wdio-allure-reporter": "^0.6.0",
    "wdio-cucumber-framework": "^2.2.7",
    "wdio-dot-reporter": "0.0.5",
    "wdio-json-reporter": "^0.1.1",
    "wdio-junit-reporter": "0.0.2",
    "wdio-spec-reporter": "^0.1.2",
    "webdriverio": "^4.8.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "cucumber-html-reporter": "^4.0.4",
    "glob": "^7.1.3",
    "install": "^0.12.1",
    "selenium-standalone": "^6.15.3",
    "wdio-allure-reporter": "^0.8.2",
    "wdio-cucumber-reporter": "0.0.2",
    "wdio-dot-reporter": "0.0.10",
    "wdio-iedriver-service": "^0.1.0",
    "wdio-json-reporter": "^0.3.1",
    "wdio-junit-reporter": "^0.4.4",
    "wdio-mocha-framework": "^0.6.3",
    "wdio-multiple-cucumber-html-reporter": "^0.3.2",
    "wdio-selenium-standalone-service": "0.0.10",
    "wdio-spec-reporter": "^0.1.5",
    "webdriverio": "^4.13.2"
  }
}
